{
  "name": "vortezzdev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "build": "concurrently 'npm:build:*'",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "dev:client": "node ace build:client --watch './resources/!(views)/**/*.ts(x)?'",
    "dev:server": "node ace serve --watch",
    "build:client": "node ace build:client --production",
    "build:server": "node ace build --production"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.0",
    "@babel/preset-react": "^7.18.6",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "adonis-preset-ts": "^2.1.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-adonis": "^2.1.0",
    "pino-pretty": "^8.1.0",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/i18n": "^1.5.6",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/view": "^6.1.6",
    "@microeinhundert/radonis-unocss": "^1.7.2",
    "@types/socket.io": "^3.0.2",
    "axios": "^0.27.2",
    "luxon": "^3.0.1",
    "mysql": "^2.18.1",
    "proxy-addr": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.1",
    "source-map-support": "^0.5.21"
  }
}
